<div class="headerCont">
    <a href="/"><h1>QuickQuips</h1></a>
    <div class="greetingCont">
    <% if(locals.user) {%>
        <h3><%= user.username%></h3>
            <img src="<%= !user.membership ? '/person.png' : '/membership.png' %>" class="member badge">
            <% if(user.admin === true) {%>
                <img src="/starpin.png" alt="admin badge" class="admin badge">
            <% } %>
        <% } else { %>
            <h5>Please <a href="/log-in">sign in</a> to continue</h5>
        <% } %>
    </div>
</div>